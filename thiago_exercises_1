print("SEQUENCIAL------------------------------")
x = 8
y = 5

print(f"A soma é {x+y}")
print(f"A multiplicação é {x*y}")

print("CONDICIONAL------------------------------")

if x == y:
    print("É igual")
else:
    print("Não é igual")
    
print("LOOPS------------------------------")

for i in range(2,5):
 print(f"X + {i} = {x+i}")
 
for j in "Batman":
    print(f"Batman tem a letra '{j}' ")
    
b = True
c = 0
while b:
    if c+x == 10:
        print ("SIM")
        b = False
    else:
        print("NÃO")
        c +=1
    
