# QUestão 5

matriz = [[], []]
for i in range(2):
    for j in range(2):
        n = int(input("Digite um número: "))
        matriz[i].append(n)
        
total = 0
for i in range(2):
    for j in range(2):
        total += matriz[i][j]

print(f"O total é {total}")
        






